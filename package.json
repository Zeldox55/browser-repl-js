  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-livereload": "^1.0.0",
