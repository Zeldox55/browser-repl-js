    "prepublishOnly": "npm run srcdoc && npm run build",
    "srcdoc": "node scripts/build-srcdoc.js",
    "codemirror": "cp node_modules/codemirror/lib/codemirror.js test/public/ && cp node_modules/codemirror/lib/codemirror.css test/public/ && cp node_modules/codemirror/mode/javascript/javascript.js test/public/ ",
    "local": "run-p start:dev autobuild"
  },
  "dependencies": {
    "codemirror": "^5.47.0"
  },
